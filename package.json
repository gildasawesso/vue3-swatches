{
  "name": "vue3-swatches",
  "version": "1.1.2",
  "description": "ðŸŽ¨ Vue 3 swatch picker",
  "keywords": [
    "vue",
    "swatch"
  ],
  "main": "dist/vue3-swatches.ssr.js",
  "browser": "dist/vue3-swatches.esm.js",
  "module": "dist/vue3-swatches.esm.js",
  "unpkg": "dist/vue3-swatches.min.js",
  "types": "./swatch.d.ts",
  "exports": {
    ".": {
      "require": "./dist/vue3-swatches.ssr.js",
      "import": "./dist/vue3-swatches.esm.js"
    },
    "./nuxt": {
      "import": "./nuxt/module.mjs"
    }
  },
  "files": [
    "dist/*",
    "./swatch.d.ts",
    "nuxt"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "release": "bumpp --commit --push --tag && pnpm publish"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@nuxt/kit": "3.0.0-rc.13",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.45",
    "bumpp": "^7.1.1",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "engines": {
    "node": ">=16"
  }
}
